{
    "manifest_version": 2,
    "name": "Data Loader",
    "version": "0.3",
    "content_scripts": [
        {
            "matches": [
                "http://localhost:5500/*"
            ],
            "js": [
                "jquery.min.js",
                "content.js"
            ]
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "page_action": {
        "default_popup": "popup.html"
    }
}